{
  "name": "blocks",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "bun run test:unit -- --run && bun run test:e2e",
    "test:e2e": "playwright test",
    "test:update-snapshots": "playwright test e2e/*snapshot* --update-snapshots --update-source-method=overwrite"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "3.1.0",
    "@eslint/compat": "1.3.2",
    "@eslint/js": "9.34.0",
    "@floating-ui/dom": "1.7.4",
    "@lucide/svelte": "0.542.0",
    "@oddbird/css-anchor-positioning": "0.6.1",
    "@playwright/test": "1.55.0",
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/kit": "2.37.0",
    "@sveltejs/vite-plugin-svelte": "6.1.4",
    "@testing-library/svelte": "5.2.8",
    "@types/node": "24.3.0",
    "@vitest/browser": "3.2.4",
    "chrono-node": "2.8.4",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-svelte": "3.11.0",
    "globals": "16.3.0",
    "jazz-tools": "0.18.4",
    "jiti": "2.5.1",
    "open-props": "1.7.16",
    "playwright": "1.55.0",
    "postcss-custom-media": "11.0.6",
    "postcss-jit-props": "1.0.16",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.38.6",
    "svelte-check": "4.3.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0",
    "vite": "7.1.4",
    "vite-plugin-devtools-json": "1.0.0",
    "vitest": "3.2.4",
    "vitest-browser-svelte": "1.1.0"
  }
}
